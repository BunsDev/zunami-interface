{
    "overrides": [
        {
            "files": ["*.js", "*.ts", "*.tsx", "*.scss"],
            "options": {
                "printWidth": 100,
                "tabWidth": 4,
                "useTabs": false,
                "singleQuote": true,
                "bracketSpacing": true,
                "explicitTypes": "always"
            }
        }
    ]
}
